#include <stdio.h>
#include <stdlib.h>
#include <pthread.h>

void *send(void) {
printf("## consumer terminating...\n");

printf("I am the central. PID = [%d] PPID = [%d]\n",getpid(),getppid());


}

void main( void )
{
	printf("Circular buffer Example\n");

	pthread_t th1;

	pthread_create(&th1, NULL, send, NULL);
	pthread_join(th1, NULL);

	printf("## main terminating...\n");
}

